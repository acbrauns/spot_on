


    <form class="create_event" action="/events/create" method="post">
    <input placeholder="Name" type="text" name="title"><br>
    <input placeholder="Start Date" type="text" name="start_date"><br>
    <input placeholder="End Date" type="text" name="end_date"><br>
    <input placeholder="Location" type="text" name="location"><br>


    <input type="submit" value="Add Event">

  </form>

  <ul class="events_list">
    <% @events.reverse_each do |event| %>
      <li><%= event.title %></li>
    <% end %>
  </ul>

